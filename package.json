{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prisma:test:generate": "prisma generate --schema=prisma/test/schema.prisma",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "migrate:deploy": "dotenv -e .env.production npx prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "test": "npm run prisma:test:generate && dotenv -e .env.test jest --detectOpenHandles --runInBand",
    "test:only": "dotenv -e .env.test jest --detectOpenHandles --runInBand",
    "test:watch": "dotenv -e .env.test jest --watch",
    "test:db:create": "dotenv -e .env.test npx prisma migrate dev --name init_test_db",
    "test:db:reset": "dotenv -e .env.test npx prisma migrate reset --force",
    "test:db:seed": "dotenv -e .env.test ts-node prisma/seed.ts",
    "preinstall": "npx npm-force-resolutions",
    "worker:image": "node dist/modules/images/image-worker.js",
    "worker:image:once": "node dist/modules/images/image-worker.js --once --max=100",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ci": "npm run build && npm run typecheck && npm run lint && npm run format:check"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20 <21"
  },
